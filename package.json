{
  "name": "sham-ui-templates",
  "version": "0.0.15",
  "description": "Template support for [sham-ui](https://github.com/shamcode/shamUI), based on [Monkberry](https://github.com/antonmedv/monkberry)",
  "bin": "bin/sham-ui-templates",
  "main": "lib/index.js",
  "browser": "./sham-ui-widget.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "browser": {
        "plugins": [
          "transform-decorators-legacy",
          "transform-class-properties",
          ["transform-es2015-modules-umd", {
            "globals": {
              "sham-ui": "shamUI"
            }
          }]
        ]
      }
    }
  },
  "scripts": {
    "test": "testem ci",
    "build:parser": "preprocess src/parser/grammar.jison ./src/parser > src/parser/grammar.full.jison && jison src/parser/grammar.full.jison -o src/parser/index.js",
    "build": "babel -d lib/ src/",
    "build:view": "BABEL_ENV=browser babel view.js -o sham-ui-widget.js",
    "watch": "babel -w -d lib/ src/",
    "prepublish": "npm run build && npm run build:view"
  },
  "license": "MIT",
  "dependencies": {
    "asciitree": "^1.0.2",
    "commander": "^2.9.0",
    "sham-ui": "1.3.3",
    "source-map": "^0.5.6"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.9.0",
    "jison": "^0.4.17",
    "phantomjs": "^2.1.7",
    "preprocessor": "^1.4.0",
    "testem": "^1.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sham-ui/sham-ui-templates.git"
  },
  "keywords": [
    "sham-ui"
  ],
  "author": "shamcode",
  "bugs": {
    "url": "https://github.com/sham-ui/sham-ui-templates/issues"
  },
  "homepage": "https://github.com/sham-ui/sham-ui-templates#readme"
}
