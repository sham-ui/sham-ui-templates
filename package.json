{
  "name": "sham-ui-templates",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "browser": "lib/view.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "browser": {
        "plugins": [
          "transform-decorators-legacy",
          "transform-class-properties",
          "transform-es2015-modules-umd"
        ]
      }
    }
  },
  "scripts": {
    "test": "testem ci",
    "build:parser": "preprocess src/parser/grammar.jison ./src/parser > src/parser/grammar.full.jison && jison src/parser/grammar.full.jison -o src/parser/index.js",
    "build": "babel -d lib/ src/",
    "build:view": "BABEL_ENV=browser babel shamui-view.js -o lib/view.js",
    "watch": "babel -w -d lib/ src/",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "asciitree": "^1.0.2",
    "commander": "^2.9.0",
    "sham-ui": "1.3.0",
    "source-map": "^0.5.6"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.9.0",
    "jison": "^0.4.17",
    "phantomjs": "^2.1.7",
    "preprocessor": "^1.4.0",
    "testem": "^1.18.0"
  }
}
